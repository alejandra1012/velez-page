{
  "flex-layout.row#footer-container.desktop": {
    "title": "footer desktop",
    "children": ["flex-layout.col#footer-subcontainer.desktop"],
    "props": {
      "marginBottom": 5,
      "paddingBottom": 8
    }
  },
  "flex-layout.col#footer-subcontainer.desktop": {
    "children": [
      "flex-layout.row#footer-container__contact",
      "flex-layout.row#footer-container__menu",
      "flex-layout.col#footer-container__info"
    ],
    "props": {
      "paddingLeft": 7,
      "paddingRight": 7
    }
  },
  "flex-layout.row#footer-container__contact": {
    "children": ["rich-text#footer__top-contact"],
    "props": {
      "blockClass": "footer-container__contact"
    }
  },
  "rich-text#footer__top-contact": {
    "props": {
      "text": "**Línea de servicio al cliente \n 01 8000 114 000**",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "flex-layout.col#footer-container__info": {
    "children": ["image#footer__banner--sic", "rich-text#footer__bottom--info"],
    "props": {
      "width": "auto",
      "rowGap": "2",
      "blockClass": "footer-container__info"
    }
  },
  "image#footer__banner--sic": {
    "props": {
      "src": "assets/image/industria y comercio.svg",
      "alt": "icon-sic",
      "maxWidth": 250
    }
  },
  "rich-text#footer__bottom--info": {
    "props": {
      "text": "TODOS LOS DERECHOS RESERVADOS CUEROS VELÉZ S.A.S \n NOTIFICACIONES JUDICIALES: CL 29 # 52 - 115 MEDELLÍN COLOMBIA| cvelezm@cuerosvelez.com +604 355 5100 | NIT 800191700-8",
