{
  "header-row#header__bottom--desktop": {
    "title": "haeder bottom desktop",
    "children": ["flex-layout.row#header__bottom--desktop"],
    "props": {
      "sticky": true,
      "blockClass": "header__bottom--desktop"
    }
  },
  "flex-layout.row#header__bottom--desktop": {
    "children": [
      "flex-layout.row#header__bottom--menu",
      "flex-layout.col#header__bottom--logo",
      "flex-layout.row#header__bottom--container"
    ],
    "props": {
      "horizontalAlign": "center",
      "blockClass": "header__bottom--desktop"
    }
  },
  "flex-layout.row#header__bottom--menu": {
    "children": ["menu#header__bottom--menu"],
    "props": {
      "blockClass": "header__bottom--menu"
    }
  },
  "flex-layout.col#header__bottom--logo": {
    "children": ["logo#header__bottom--logo"]
  },
  "logo#header__bottom--logo": {
    "props": {
      "href": "/",
      "url": "assets/image/logo_velez.svg",
      "width": 140,
      "height": 30,
      "blockClass": "header__bottom--logo"
    }
  },
  "flex-layout.row#header__bottom--container": {
    "children": [
      "search-bar#header__bottom--search",
      "login#header__bottom--login",
      "minicart.v2#header__bottom--minicat"
    ],
    "props": {
      "horizontalAlign": "right",
      "blockClass": "header__bottom--container"
    }
  },
  "login#header__bottom--login": {
    "props": {
      "showIconProfile": true,
      "hideIconLabel": true,
      "blockClass": "header__bottom--login"
    }
  }
}
